{
    "name": "blanketsmith-monorepo",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev:tool": "npm run dev -w @blanketsmith/tool",
        "dev:web": "npm run dev -w @blanketsmith/web",
        "build": "npm run build -w @blanketsmith/tool && npm run build -w @blanketsmith/web"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}